ate-life-page",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_ADMIN_DELETE_LIFE_PAGE:new t.default(i.ADMIN_LIFE_PAGE,"delete-life-page",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_ADMIN_UNPUBLISH_LIFE_PAGE:new t.default(i.ADMIN_LIFE_PAGE,"unpublish-life-page",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_ADMIN_JOBS_PAGE:new t.default(i.ADMIN_JOBS,"admin-jobs-page",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_ADMIN_JOBS_PAGE_DISPLAY:new t.default(i.ADMIN_JOBS,"admin-jobs-page-display",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_ADMIN_JOBS_PAGE_EDIT:new t.default(i.ADMIN_JOBS,"admin-jobs-page-edit",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_TALENT_BRAND_ANALYTICS_SUMMARY_SECTION:new t.default(i.ADMIN_TALENT_BRAND_ANALYTICS,"analytics-summary-section",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_TALENT_BRAND_ANALYTICS_TALENT_FLOW_SECTION:new t.default(i.ADMIN_TALENT_BRAND_ANALYTICS,"talent-flow-section",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_TALENT_BRAND_ANALYTICS_APPLICANT_BEHAVIOR_SECTION:new t.default(i.ADMIN_TALENT_BRAND_ANALYTICS,"applicant-behavior-section",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN}),LCP_TALENT_BRAND_ANALYTICS_TALENT_ATTRACTION_SECTION:new t.default(i.ADMIN_TALENT_BRAND_ANALYTICS,"talent-attraction-section",{productName:n.VOYAGER_ORGANIZATION_LCP_ADMIN})}
